{
  "$id": "natureOfContent.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Nature of content",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "title": "Nature of content URLI",
        "type": "string",
        "format": "uri",
        "pattern": "^https:\/\/d-nb.info\/gnd\/1[012]?\\d{7}[0-9X]|[47]\\d{6}-\\d|[1-9]\\d{0,7}-[0-9X]|3\\d{7}[0-9X]"
      },
      "label": {
        "description": "The human-readable German label for the nature of content",
        "type": "string"
      }
    },
    "required": [
      "label",
      "id"
    ]
  }
}
