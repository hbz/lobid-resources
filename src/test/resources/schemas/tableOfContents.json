{
  "$id": "tableOfContents.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Table of contents",
  "description": "A link to a scanned table of contents",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "title": "ToC URL",
        "type": "string",
        "format": "uri"
      },
      "label": {
        "description": "ToC label",
        "type": "string"
      }
    },
    "required": [
      "id",
      "label"
    ]
  }
}
