{
    "$id": "primaryForm.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Primary Form",
    "description": "The resource that has been digitized",
    "type": "array",
    "items": {
        "type": "object",
        "additionalProperties":false,
        "properties": {
            "id": {
                "type": "string",
                "format": "uri"
            },
            "label": {
                "type": "string"
            },
            "note": {
              "title": "Note",
              "type": "array",
              "items": {
                "type": "string"
              }
            }
        }
    }
}
