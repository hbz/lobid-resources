{
  "$id": "accessRights.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Open Access Marker",
  "description": "This is a marker if resource is open access. It does not state if a resource is not due to missing definitive info in the MARC data.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "format": "uri",
      "enum": [
        "http://purl.org/coar/access_right/c_abf2"
      ]
    },
    "label": {
      "type": "string",
      "enum": [
        "Open Access"
      ]
    }
  },
  "required": [
    "id",
    "label"
  ]
}
