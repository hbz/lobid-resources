{
    "$id": "webPageArchived.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Web page that is archived",
    "description": "Link to the web page that the ressource is a archived version of",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "format": "uri"
            },
            "label": {
                "type": "string"
            }
        },
        "required": [
          "id",
          "label"
        ]
    }
}
