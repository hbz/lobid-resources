{
    "$id": "bibliographicLevel.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "MARC21 Bibliographic Leader07 - Bibliographic level",
    "description": "URI for the same resource elswehere",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "format": "uri",
            "pattern": "https://www.loc.gov/marc/bibliographic/bdleader.html#.*"
        },
        "label": {
            "type": "string",
            "enum": [
              "Monographic component part",
              "Serial component part",
              "Collection",
              "Subunit",
              "Integrating resource",
              "Monograph/Item",
              "Serial"
            ]
        }
    }
}


