{
  "$id": "containedIn.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contained in",
  "description": "The resource, mostly an article, is contained in another lobid resource.",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "string",
        "format": "uri",
        "pattern": "http://lobid.org/resources/.+"
      },
      "label": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "label"
    ]
  }
}
