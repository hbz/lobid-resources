{
  "$id": "manufacture.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Manufacture statements",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "type": {
        "type": "array",
        "items": {
          "type": "string",
          "const": "Event"
        }
      },
      "location": {
        "title": "Place of manufacture",
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "manufacturedBy": {
        "title": "Agent of manufacture",
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "dateStatement": {
        "title": "Manufacture date as catalogued",
        "type": "string"
      },
      "startDate": {
        "title": "(Start) date of the manufacture",
        "type": "string"
      },
      "endDate": {
        "title": "End date of the manufacture",
        "type": "string"
      }
    },
    "required": [
      "type"
    ]
  }
}
