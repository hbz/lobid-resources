{
    "$id": "containsExampleOfWork.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Contains example of work",
    "type": "array",
    "items": {
        "type": "object",
        "additionalProperties":false,
        "properties": {
            "id": {
                "type": "string",
                "format": "uri"
            },
            "label": {
                "type": "string"
            },
            "creatorOfWork": {
                "type": "string"
            },
            "type": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "Work"
                ]
              }
            },
            "instrumentation": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "musicalKey": {
                "type": "string"
            },
            "workNumbering": {
                "type": "string"
            }
        }
    }
}
