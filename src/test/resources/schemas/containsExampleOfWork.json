{
  "$id": "containsExampleOfWork.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contains example of work",
  "description": "Item in hand contains the work that is represented by the added entry in MARC 700/710/711/730 with Ind2=2.",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "string",
        "format": "uri",
        "pattern": "^https:\/\/d-nb.info\/gnd\/1[012]?\\d{7}[0-9X]|[47]\\d{6}-\\d|[1-9]\\d{0,7}-[0-9X]|3\\d{7}[0-9X]"
      },
      "label": {
        "type": "string"
      },
      "creatorOfWork": {
        "type": "string"
      },
      "type": {
        "type": "array",
        "items": {
          "type": "string",
          "enum": [
            "Work"
          ]
        }
      },
      "instrumentation": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "musicalKey": {
        "type": "string"
      },
      "workNumbering": {
        "type": "string"
      }
    },
    "required": [
      "label"
    ]
  }
}
