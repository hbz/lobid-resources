{
  "settings": {
    "index": {
      "number_of_shards": 3,
      "analysis": {
        "analyzer": {
          "german_analyzer": {
            "filter": [
              "lowercase",
              "snowball"
            ],
            "tokenizer": "standard",
            "type": "custom"
          },
          "german_analyzer_unstemmed": {
            "filter": [
              "lowercase",
              "german_normalization"
            ],
            "tokenizer": "standard",
            "type": "custom"
          },
          "id_analyzer": {
            "tokenizer": "keyword",
            "filter": [
              "lowercase"
            ]
          },
          "ngram_analyzer": {
            "filter": [
              "lowercase",
              "stop",
              "ngram",
              "snowball"
            ],
            "tokenizer": "letter",
            "type": "custom"
          },
          "hyphen_analyzer": {
            "tokenizer": "standard",
            "char_filter": "remove-hyphens"
          },
          "digibib_default": {
            "type": "custom",
            "tokenizer": "hyphen",
            "filter": [
              "lowercase",
              "digibib_german_stop_words",
              "keyword_repeat",
              "icu_normalizer",
              "icu_folding",
              "hyphen",
              "digibib_string_normalizer",
              "snowball",
              "digibib_standardnumber",
              "unique"
            ]
          },
          "digibib_search": {
            "type": "custom",
            "tokenizer": "hyphen",
            "filter": [
              "lowercase",
              "digibib_german_stop_words",
              "keyword_repeat",
              "icu_normalizer",
              "icu_folding",
              "digibib_string_normalizer",
              "snowball",
              "digibib_standardnumber",
              "unique"
            ]
          },
          "digibib_unstemmed": {
            "type": "custom",
            "tokenizer": "hyphen",
            "filter": [
              "lowercase",
              "digibib_german_stop_words",
              "keyword_repeat",
              "icu_normalizer",
              "icu_folding",
              "digibib_simple_hyphen",
              "german_normalize",
              "unique"
            ]
          }
        },
        "filter": {
          "ngram": {
            "min_gram": 1,
            "type": "edgeNGram",
            "max_gram": 40
          },
          "snowball": {
            "type": "snowball",
            "language": "German2"
          },
          "digibib_german_stop_words": {
            "type": "stop",
            "stopwords": [
              "and",
              "the",
              "of",
              "to",
              "als",
              "am",
              "an",
              "auf",
              "aus",
              "bei",
              "bis",
              "da",
              "das",
              "dass",
              "daß",
              "dem",
              "den",
              "der",
              "des",
              "die",
              "dir",
              "du",
              "durch",
              "ein",
              "eine",
              "einem",
              "einen",
              "einer",
              "eines",
              "er",
              "es",
              "für",
              "hat",
              "ihre",
              "im",
              "in",
              "ist",
              "mit",
              "nach",
              "nicht",
              "oder",
              "um",
              "und",
              "vom",
              "von",
              "vor",
              "was",
              "wie",
              "zu",
              "zum",
              "zur",
              "über"
            ]
          },
          "digibib_simple_hyphen": {
            "type": "hyphen",
            "subwords": false
          },
          "digibib_standardnumber": {
            "type": "standardnumber",
            "standardnumbers": [
              "isbn"
            ]
          },
          "digibib_string_normalizer": {
            "type": "pattern_replace",
            "pattern": "'",
            "replacement": ""
          }
        },
        "char_filter": {
          "remove-hyphens": {
            "type": "pattern_replace",
            "pattern": "-",
            "replacement": ""
          }
        }
      }
    }
