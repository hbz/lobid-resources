   "settings" : {
      "index" : {
         "number_of_shards" : 3,
         "analysis" : {
            "analyzer" : {
               "german_analyzer" : {
                  "filter" : [
                     "lowercase",
                     "snowball"
                  ],
                  "tokenizer" : "standard",
                  "type" : "custom"
               },
               "german_analyzer_unstemmed" : {
                  "filter" : [
                     "lowercase",
                     "german_normalization"
                  ],
                  "tokenizer" : "standard",
                  "type" : "custom"
               },
               "id_analyzer" : {
                  "tokenizer" : "keyword",
                  "filter" : [
                     "lowercase"
                  ]
               },
               "ngram_analyzer" : {
                  "filter" : [
                     "lowercase",
                     "stop",
                     "ngram",
                     "snowball"
                  ],
                  "tokenizer" : "letter",
                  "type" : "custom"
               },
               "hyphen_analyzer": {
                  "tokenizer": "standard",
                  "char_filter": "remove-hyphens"
               }
            },
            "filter" : {
               "ngram" : {
                  "min_gram" : 1,
                  "type" : "edgeNGram",
                  "max_gram" : 40
               },
               "snowball" : {
                  "type" : "snowball",
                  "language" : "German2"
               }
            },
            "char_filter": {
               "remove-hyphens": {
                 "type": "pattern_replace",
                 "pattern": "-",
                 "replacement": ""
               }
            }
         }
      }
