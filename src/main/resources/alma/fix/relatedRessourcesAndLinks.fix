        copy_field("$label", "related[].$last.label")
        add_array("related[].$last.issn[]")
        copy_field("$issn", "related[].$last.issn[].$append")
        add_array("related[].$last.isbn[]")
        isbn("$isbn", to:"isbn13")
        copy_field("$isbn", "related[].$last.isbn[].$append")
        isbn("$isbn", to:"isbn10")
        copy_field("$isbn", "related[].$last.isbn[].$append")
      end
    end
  end
end

