add_array("related[]")
do list(path:"775??|776??", "var":"$i")
  unless any_match ("$i.i", ".*eproduktion von.*|.*eproduktion")
    do list_as("$hbzId":"$i.w", "$isbn":"$i.z", "$label":"$i.t", "$note":"$i.n", "$issn":"$i.x", "$info":"$i.i")
      unless in("$isbn", "related[].*.isbn[]") # Skip duplicates
